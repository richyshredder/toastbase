{
  "name": "phaser-typescript-webpack",
  "version": "1.0.0",
  "description": "A good place to get started with Phaser and TypeScript development",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.config.js -d --inline --hot --progress --host 0.0.0.0 --port 3000 --mode development",
    "build": "rimraf ./build && webpack --config webpack.prod.config.js -p --progress --mode production",
    "lint": "tslint src/**/*.ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/numbofathma/phaser-typescript-webpack.git"
  },
  "engines": {
    "node": ">= 7.0.0"
  },
  "author": "Costin Mirica <hi@costinmirica.com> (http://www.costinmirica.com/)",
  "license": "MIT",
  "homepage": "https://github.com/numbofathma/phaser-typescript-webpack",
  "devDependencies": {
    "@types/lodash": "^4.14.134",
    "copy-webpack-plugin": "^5.0.3",
    "expose-loader": "^0.7.5",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.4.5",
    "tslint": "^5.16.0",
    "tslint-loader": "^3.5.4",
    "tslint-webpack-plugin": "^2.0.4",
    "typescript": "^3.4.5",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-obfuscator": "^0.18.0",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "@types/howler": "^2.1.0",
    "dotenv-webpack": "^1.7.0",
    "firebase": "^6.2.0",
    "howler": "^2.1.2",
    "phaser-ce": "^2.12.0"
  }
}
